<!-- Section: Design Block -->
<main id="main">
  <section>
    <div class="container">
      <div class="row">
        <div class="col-auto px-0">
          <div id="sidebar" class="collapse collapse-horizontal show border-end">
            <div
              id="sidebar-nav"
              style="width: 160px"
              class="list-group border-0 rounded-0 text-sm-start"
            >
              <a
                href="/admin/posts"
                class="list-group-item border-end-0 d-inline-block text-truncate"
                data-bs-parent="#sidebar"
                ><i class="bi bi-bootstrap"></i> <span>Posts</span>
              </a>
              <a
                href="/admin/categories"
                class="list-group-item border-end-0 d-inline-block text-truncate"
                data-bs-parent="#sidebar"
                ><i class="bi bi-film"></i> <span>Categories</span></a
              >
              <a
                href="/admin/users"
                class="list-group-item border-end-0 d-inline-block text-truncate"
                data-bs-parent="#sidebar"
                ><i class="bi bi-heart"></i> <span>Users</span></a
              >
            </div>
          </div>
        </div>
        <div class="col pt-2">
          <a
            href="#"
            data-bs-target="#sidebar"
            data-bs-toggle="collapse"
            class="border rounded-3 p-1 text-decoration-none"
            ><i class="bi bi-list bi-lg py-2 p-1"></i> Menu</a
          >
          <div class="container" data-aos="fade-up">
            <div class="row">
              <div class="col-md-12">
                <!-- ======= #main ======= -->
                <%- include(`../../pages/${templates[1]}`) %>
                <!-- End #main -->
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
</main>
<!-- Section: Design Block -->
